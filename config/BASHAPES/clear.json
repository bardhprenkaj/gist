{  
    "experiment": {
        "scope": "bashapes",
        "parameters" : {
            "lock_release_tout":120,
            "propagate":[
                {"in_sections" : ["explainers"],"params" : {"fold_id": 0}},
                {"in_sections" : ["do-pairs/oracle"],"params" : {"fold_id": -1,"retrain": false}},
                {"in_sections": ["do-pairs/dataset"],"params": { "manipulators" : 
                [{ "class": "src.dataset.manipulators.causality.Causality", "parameters": {} }] }}
            ]
        }
    },

    "do-pairs":[ {"compose_tc28": "config/snippets/do-pairs/BASHAPES-500-300-5_80_GCN.json"} ],
    "explainers": [
        {
            "class": "src.explainer.generative.clear.CLEARExplainer",
            "parameters": {
                "epochs": 50,
                "lr": 0.07,
                "lambda_cfe": 0.1,
                "alpha": 0.4,
                "batch_size": 32
            }
        }
    ],
    "compose_mes" : "config/snippets/default_metrics_w_dumper.json",
    "compose_strs" : "config/snippets/default_store_paths.json"
}